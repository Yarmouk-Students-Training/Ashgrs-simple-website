<html lang="en">
    <%- include('./partials/head.ejs')%>
<body>
    <%- include('./partials/nav.ejs')%>
    <div class="blog-content">
        <h2 class ='main'>Blogs</h2>
        <% if (blogs.length > 0) { %>
            <% blogs.forEach(blog => { %>
              <h3 class="title"><%= blog.blogTitle %></h3>
              <p class="content"><%= blog.content %></p>
            <% }) %>
        <% }else{%>
            <h4>No Blogs Yet</h4>
        <%}%>
    </div>
    <%- include('./partials/footer.ejs')%>
</body>
</html>